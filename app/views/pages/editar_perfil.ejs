<!DOCTYPE html>
<html lang="pt-br">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">

    <title>Editar Perfil</title>

    <!-- LINKS CSS -->
    <link rel="stylesheet" href="css/reset.css">
    <link rel="stylesheet" href="css/default.css">
    <link rel="stylesheet" href="css/styleCadVisitante.css">

    <!-- LINKS PARA O GOOGLE FONTS -->
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>

    <!-- FONTS -->
    <!-- GOTHIC-A1 -->
    <link href="https://fonts.googleapis.com/css2?family=Gothic+A1&display=swap" rel="stylesheet">
    <!-- SORA -->
    <link href="https://fonts.googleapis.com/css2?family=Roboto:wght@300&family=Sora:wght@400;600&display=swap"
        rel="stylesheet">

    <!-- LINKS JS -->
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.6.0/jquery.min.js"></script>
    <script src="js/script_cadastro.js"></script>
    <script src="js/mask.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery.mask/1.14.16/jquery.mask.min.js"></script>
</head>

<body>
    <header id="normal_header">
        <div id="fixed">
            <a href="index.html"><img src="img/Logo Oficinas Web 1 1.svg" alt="" class="logotipo"></a>

        </div>
    </header>

    <div id="topo" class="topo"></div>

    <main class="editar_perfil">
        <section class="cad_visitante">
            <div class="titulo_arrow">
                <img src="img/arrow_left.svg" alt="logo">
                <h1 class="titulo">Editar Perfil</h1>
            </div>

            <div class="foto foto-editavel">
                <div id="inputs_foto">
                    <div class="circle_foto">
                        <img src="img/foto-padrao.svg" alt="" id="foto_padrao_perfil-f">
                    </div>
                    <!-- <div id="foto"></div> -->
                    <label for="add-img-f" id="addinput"><img src="img/add-foto.svg" alt="logo"></label>
                    <input type="file" name="add-img" id="add-img-f">
                </div>
            </div>

            <div>
                <form action="nome">
                    <input id="nome" class="inputs" placeholder="Nome" type="text">
                    <!-- <input id="cpf" class="inputs" placeholder="CPF" type="text"> -->
                    <div>
                        <input id="celular" class="inputs" placeholder="Celular" type="text">
                        <input id="email" class="inputs" placeholder="Email" type="text">
                    </div>
                </form>

                <div class="cadastro">
                    <span><a href="login.html"> Alterar Senha </a></span>
                </div>

                <button class="btn default_btn"> <span>Continuar</span> </button>
            </div>
        </section>
    </main>


</body>

</html>